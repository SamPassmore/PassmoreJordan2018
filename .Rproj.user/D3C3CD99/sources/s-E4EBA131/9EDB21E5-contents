# plot trees
library(ape)
library(dplyr)

d = read.table('data/indoeuropean2.btdata')
tr = read.nexus('data/indoeuropean.bttrees')

kincodes = read.csv('kincode.csv')

d = left_join(d, kincodes, by = c("V2" = "kin.number"))
rownames(d) = d$V1
d = d[tr[[1]]$tip.label,]

plot(tr[[1]])
tiplabels(pch = 19, col = d$V2)



